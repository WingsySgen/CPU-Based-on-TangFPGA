LABEL MAIN
MOV|IM1 1 _ REG13
LABEL LOOP
    JNE|IM2 TIMER 0 LOOP
    CALL _ _ FUNCTION
    LABEL WAIT1
        JE|IM2 TIMER 0 WAIT1
JE _ _ LOOP

LABEL FUNCTION
    MULL|IM2 REG13 2 REG1
    JNE|IM2 REG1 16 NXT1
        MOV|IM1 1 _ REG13
        RET _ _ _
    LABEL NXT1
        MOV REG1 _ REG13
        RET _ _ _